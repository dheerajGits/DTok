type balanceTypeWithMessage = 
 record {
   balance: nat;
   isFreeTokensRecieved: bool;
   message: text;
 };
service : {
  balanceOf: (principal) -> (nat) query;
  findData: (principal) -> (balanceTypeWithMessage) query;
  getCurrencySymbol: () -> (text) query;
  payOut: () -> (text);
  transfer: (principal, nat) -> (text);
}
